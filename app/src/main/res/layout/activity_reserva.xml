<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Reserva">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="338dp"
        android:layout_height="152dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        app:srcCompat="@drawable/ml" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="169dp"
        android:layout_marginEnd="22dp"
        android:text="20/Oct/2018"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView28"
        android:layout_alignStart="@+id/imageView9"
        android:text="Codigo:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="238dp"
        android:text="Cliente:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="259dp"
        android:text="Correo:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="217dp"
        android:text="Fecha:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="179dp"
        android:text="Disponibilidad:"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageView9"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="97dp"
        android:text="Total: "
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView37"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="140dp"
        android:text="Mesa: "
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btnreservaa"
        android:layout_width="339dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView37"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="17dp"
        android:background="?attr/colorBackgroundFloating"
        android:onClick="btnreservaA"
        android:text="reservar "
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView29"
        android:layout_alignParentStart="true"
        android:layout_marginStart="106dp"
        android:text="01"
        android:textSize="20sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/editText10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView39"
        android:layout_alignTop="@+id/textView30"
        android:layout_marginStart="-6dp"
        android:layout_marginTop="-9dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/editText11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editText10"
        android:layout_alignStart="@+id/editText10"
        android:ems="10"
        android:inputType="textEmailAddress"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/editText12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView36"
        android:layout_alignStart="@+id/editText10"
        android:layout_marginStart="-2dp"
        android:layout_marginBottom="2dp"
        android:ems="10"
        android:inputType="date"
        android:textSize="16sp" />

    <EditText
        android:id="@+id/editText13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView37"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="-3dp"
        android:layout_toEndOf="@+id/textView34"
        android:ems="10"
        android:inputType="numberDecimal"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="23dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/textView39"
        android:layout_alignBottom="@+id/textView38"
        android:text="5"
        android:textSize="20sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="50dp"
        android:layout_height="18dp"
        android:layout_above="@+id/textView38"
        android:layout_alignParentStart="true"
        android:layout_marginStart="165dp"
        android:layout_marginBottom="17dp"
        app:srcCompat="@drawable/fbg" />
</RelativeLayout>